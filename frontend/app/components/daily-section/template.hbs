<li class="collection-item purple darken-1 white-text">
  <h4>
    {{content.label}}
    <a class="btn-floating waves-effect waves-light deep-orange darken-1 right">
      <i class="mdi-content-create"></i>
    </a>
  </h4>
</li>

{{#each content.dailyTasks as |task|}}
  <li class="collection-item blue lighten-5">
    {{task.label}}
  </li>
{{else}}
  <li class="collection-item purple lighten-5">none</li>
{{/each}}
